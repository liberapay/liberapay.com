msgid_plural "Liberapay was launched {timespan_ago} and has {n} users."
msgstr[0] "Liberapay запущено {timespan_ago} і він зараз налічує {n} користувача."
msgstr[1] "Liberapay запущено {timespan_ago} і він зараз налічує {n} користувача."
msgstr[2] "Liberapay запущено {timespan_ago} і він зараз налічує {n} користувачів."

#, python-brace-format
msgid "<unused singular (hash=fd1709b5b191a6dcb851e261d315f1e1)>"
msgid_plural "The last payday was {timespan_ago} and transferred {money_amount} between {n} users."
msgstr[0] "Останній день сплати був {timespan_ago}, було переказано {money_amount} {n} користувачу."
msgstr[1] "Останній день сплати був {timespan_ago}, було переказано {money_amount} між {n} користувачами."
msgstr[2] "Останній день сплати був {timespan_ago}, було переказано {money_amount} між {n} користувачами."

#, python-brace-format
msgid "{n} participant gave money."
msgid_plural "{n} participants gave money."
msgstr[0] "{n} учасник віддав гроші."
msgstr[1] "{n} учасники віддали гроші."
msgstr[2] "{n} учасників віддали гроші."

