msgid_plural "Liberapay was launched {timespan_ago} and has {n} users."
msgstr[0] "Η Liberapay ξεκίνησε {timespan_ago} και έχει {n} χρήστη."
msgstr[1] "Η Liberapay ξεκίνησε {timespan_ago} και έχει {n} χρήστες."

#, python-brace-format
msgid "<unused singular (hash=fd1709b5b191a6dcb851e261d315f1e1)>"
msgid_plural "The last payday was {timespan_ago} and transferred {money_amount} between {n} users."
msgstr[0] "<unused singular (hash=fd1709b5b191a6dcb851e261d315f1e1)>"
msgstr[1] "Η τελευταία ημέρα πληρωμής ήταν {timespan_ago} και μεταφέρθηκε {money_amount} μεταξύ {n} χρηστών."

#, python-brace-format
